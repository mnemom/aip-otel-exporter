{
  "name": "@mnemom/aip-otel-exporter",
  "version": "0.1.0",
  "description": "OpenTelemetry exporter for AIP integrity checkpoints and AAP verification results",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auto": {
      "import": "./dist/auto/index.js",
      "types": "./dist/auto/index.d.ts"
    },
    "./workers": {
      "import": "./dist/workers/index.js",
      "types": "./dist/workers/index.d.ts"
    }
  },
  "files": [
    "dist",
    "dashboards"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "bench": "vitest bench",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@opentelemetry/api": ">=1.4.0"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-metrics": "^1.30.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@types/node": "^25.2.3",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "opentelemetry",
    "otel",
    "aip",
    "aap",
    "integrity",
    "alignment",
    "observability",
    "langfuse",
    "arize",
    "datadog"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mnemom/aip-otel-exporter.git",
    "directory": "packages/typescript"
  }
}
